{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "10.0.0"
    },
    {
      "type": "datasource",
      "id": "prometheus",
      "name": "Prometheus",
      "version": "1.0.0"
    }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "panels": [
    {
      "id": 1,
      "title": "Total Requests Rate",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "h": 5, "w": 4, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "sum(nginx_custom_rate_requests_5m)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "decimals": 5
        }
      }
    },
    {
      "id": 8,
      "title": "Successful Requests (2xx)",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "h": 5, "w": 4, "x": 4, "y": 0 },
      "targets": [
        {
          "expr": "sum(nginx_custom_count_requests_5m{resp_code=~\"2..\"})"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "short" } }
    },
    {
      "id": 9,
      "title": "Client Errors (4xx)",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "h": 5, "w": 4, "x": 8, "y": 0 },
      "targets": [
        {
          "expr": "sum(nginx_custom_count_requests_5m{resp_code=~\"4..\"})"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "short", "color": { "mode": "thresholds", "fixedColor": "red" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "red", "value": 1 }] } } }
    },
    {
      "id": 10,
      "title": "Server Errors (5xx)",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "h": 5, "w": 4, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "sum(nginx_custom_count_requests_5m{resp_code=~\"5..\"})"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "short", "color": { "mode": "thresholds", "fixedColor": "red" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "red", "value": 1 }] } } }
    },
    {
      "id": 17,
      "title": "P90 Latency",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "h": 5, "w": 4, "x": 16, "y": 0 },
      "targets": [
        {
          "expr": "histogram_quantile(0.90, sum(rate(nginx_request_latency_seconds_bucket[5m])) by (le))"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s", "decimals": 3 } }
    },
    {
      "id": 18,
      "title": "P95 Latency",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "h": 5, "w": 4, "x": 20, "y": 0 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(nginx_request_latency_seconds_bucket[5m])) by (le))"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s", "decimals": 3 } }
    },
    {
      "id": 5,
      "title": "Request Rate by Verb",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 5 },
      "targets": [
        {
          "expr": "sum by (verb) (nginx_custom_rate_requests_5m)",
          "legendFormat": "{{verb}}"
        }
      ],
      "fieldConfig": { "defaults": { "custom": { "stacking": { "mode": "normal" } }, "unit": "reqps", "decimals": 5 } }
    },
    {
      "id": 6,
      "title": "Request Rate by Status Code",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 5 },
      "targets": [
        {
          "expr": "sum by (resp_code) (nginx_custom_rate_requests_5m)",
          "legendFormat": "{{resp_code}}"
        }
      ],
      "fieldConfig": { "defaults": { "custom": { "stacking": { "mode": "normal" } }, "unit": "reqps", "decimals": 5 } }
    },
    {
      "id": 4,
      "title": "Request Distribution by Verb",
      "type": "piechart",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 13 },
      "targets": [
        {
          "expr": "sum by (verb) (nginx_custom_count_requests_5m)",
          "legendFormat": "{{verb}}"
        }
      ],
       "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 11,
      "title": "Request Count by Status Code",
      "type": "piechart",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 13 },
      "targets": [
        {
          "expr": "sum by (resp_code) (nginx_custom_count_requests_5m)",
          "legendFormat": "{{resp_code}}"
        }
      ],
       "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "collapsed": true,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 21
      },
      "id": 12,
      "panels": [],
      "title": "Detailed Status Code Analysis",
      "type": "row"
    },
    {
      "id": 13,
      "title": "2xx Request Count by API Path",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 22 },
      "targets": [
        {
          "expr": "sum by (request) (nginx_custom_count_requests_5m{resp_code=~\"2..\"})",
          "legendFormat": "{{request}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "short" } }
    },
    {
      "id": 14,
      "title": "3xx Request Count by API Path",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 22 },
      "targets": [
        {
          "expr": "sum by (request) (nginx_custom_count_requests_5m{resp_code=~\"3..\"})",
          "legendFormat": "{{request}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "short" } }
    },
    {
      "id": 15,
      "title": "4xx Request Count by API Path",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 30 },
      "targets": [
        {
          "expr": "sum by (request) (nginx_custom_count_requests_5m{resp_code=~\"4..\"})",
          "legendFormat": "{{request}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "short" } }
    },
    {
      "id": 16,
      "title": "5xx Request Count by API Path",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 30 },
      "targets": [
        {
          "expr": "sum by (request) (nginx_custom_count_requests_5m{resp_code=~\"5..\"})",
          "legendFormat": "{{request}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "short" } }
    }
  ],
  "schemaVersion": 37,
  "style": "dark",
  "tags": ["nginx", "requests", "logs", "alloy"],
  "templating": {
    "list": []
  },
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {},
  "timezone": "",
  "title": "Nginx Request Report",
  "uid": "nginx-request-report-alloy",
  "version": 1
}
